{
  "name": "cluster-cleanup",
  "version": "2.0.0",
  "description": "Library to fill the gap between the SDK and the CLI/Console for deleting an ECS cluster",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "nyc ava",
    "test:no-coverage": "ava",
    "open-coverage": "npx open coverage/index.html",
    "build": "tsc",
    "start": "npm run build",
    "clean": "npx rimraf dist/*.js dist/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YashdalfTheGray/cluster-cleanup.git"
  },
  "keywords": [
    "aws",
    "ecs"
  ],
  "author": "Yash Kulshrestha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YashdalfTheGray/cluster-cleanup/issues"
  },
  "homepage": "https://github.com/YashdalfTheGray/cluster-cleanup#readme",
  "devDependencies": {
    "@types/node": "^14.14.5",
    "ava": "^3.13.0",
    "nyc": "^15.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@types/lodash": "^4.14.162",
    "aws-sdk": "^2.779.0",
    "lodash": "^4.17.20"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "engines": {
    "node": ">=8.6.x",
    "npm": ">=5.6.x"
  }
}
